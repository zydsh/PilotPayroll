INSERT INTO O_TFR
	VALUES ("637e5f80-bbad-4f46-92bb-2c1f2400bbdb",
	"7ab39b34-fd7a-404b-8780-719eb8be8a17",
	'SendHoldUpdates',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select many entries from instances of PayrollElement
 where selected.HoldUpdated;
count = 0;
if ( not_empty entries )
  for each entry in entries
    select one payee related by entry->Payee[R9.''contributes to''];
    select one dept related by payee->Department[R8.''belongs to''];
    send TESTAPP::SubmitItemHold( Department:dept.Name, EmployeeID:payee.EmployeeID, PaymentLabel:entry.Label, HoldStatus:entry.Hold );
  end for;
  count = count + 1;
  send TESTAPP::UpdatesSent( Department:self.Name, Count:count );
end if;',
	1,
	'',
	"00000000-0000-0000-0000-000000000000",
	0);
INSERT INTO O_TFR
	VALUES ("ec82031d-6eea-4d52-8760-2aa5690852d8",
	"7ab39b34-fd7a-404b-8780-719eb8be8a17",
	'ClearElements',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select many payees related by self->Payee[R8.''employs''];
for each payee in payees
  select many elements related by payee->PayrollElement[R9.''aggregates''];
  for each element in elements
  	unrelate element from payee across R9.''contributes to'';
  	delete object instance element;
  end for;
  unrelate payee from self across R8.''belongs to'';
  delete object instance payee;
end for;
self.Reviewing = False;',
	1,
	'',
	"637e5f80-bbad-4f46-92bb-2c1f2400bbdb",
	0);
